<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.ui"
            class="org.example.second.views.SampleDemoView"
            id="org.example.second.view"
            name="SampleDemoView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <command
               commandId="org.example.second.command.demo"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="org.example.second.toolbar.top"
               label="TopLevelToolbar">
            <command
                  commandId="org.example.second.command.demo"
                  id="org.example.second.command.demo.toolbar.main"
                  label="Test">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="toolbar:org.eclipse.ui.console.ConsoleView">
         <command
               commandId="org.example.second.command.demo"
               id="org.example.second.command.demo.toolbar.console"
               label="Test">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.example.second.view?after=additions">
         <command
               commandId="org.example.second.command.demo"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <command
               commandId="org.example.second.command.demo"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.example.second.commands.DemoCommandHandler"
            id="org.example.second.command.demo"
            name="OpenDemoView">
      </command>
   </extension>

</plugin>
